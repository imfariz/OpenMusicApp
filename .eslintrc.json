{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "import/no-extraneous-dependencies": ["error", {"includeInternal": true, "includeTypes": true}]
    }
}
